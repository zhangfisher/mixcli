{
  "name": "mixcli",
  "version": "3.2.8",
  "description": "Develop command line tool scaffolding for monorepo",
  "repository": "https://github.com/zhangfisher/mixcli.git",
  "homepage": "https://zhangfisher.github.io/mixcli/",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "access": "public",
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "release": "changeset version && changeset publish",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "sync": "cnpm sync mixcli",
    "changeset": "changeset"
  },
  "files": [
    "dist",
    "src",
    "readme.md",
    "package.json"
  ],
  "keywords": [
    "commander",
    "prompts",
    "cli",
    "command line"
  ],
  "packageManager": "pnpm@9.0.0",
  "author": "zhangfisher",
  "license": "ISC",
  "dependencies": {
    "@types/prompts": "^2.4.9",
    "@voerkai18n/runtime": "^2.1.13",
    "art-template": "^4.13.2",
    "commander": "^13.0.0",
    "flex-tools": "^1.4.33",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.12",
    "logsets": "^1.3.31",
    "prompts": "^2.4.2",
    "string.prototype.replaceall": "^1.0.7"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.5.7",
    "tsup": "^8.3.5",
    "type-fest": "^4.32.0",
    "typescript": "^5.7.3",
    "vitepress": "^1.5.0"
  }
